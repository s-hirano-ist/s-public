{
  "name": "s-public",
  "version": "2.3.1",
  "type": "module",
  "description": "Portfolio and blog pages of s-hirano-ist.",
  "author": "s-hirano-ist",
  "packageManager": "pnpm@9.15.8",
  "scripts": {
    "astro": "astro",
    "check": "astro check",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "sync": "astro sync",
    "tsc": "tsc --noEmit",
    "fmt": "prettier --cache --check **/*.astro .",
    "fmt:fix": "prettier --cache --write **/*.astro .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:mark": "markdownlint-cli2 src/content/summary/*",
    "lint:mark:fix": "markdownlint-cli2-fix src/content/summary/*",
    "lint:css": "stylelint .",
    "lint:css:fix": "stylelint . --fix",
    "lint:secret": "secretlint --maskSecrets **/*",
    "snapshots": "playwright test",
    "snapshots:update": "playwright test --update-snapshots",
    "generate:photo": "python script/generate_photo_path.py",
    "generate:book": "node --loader ts-node/esm script/generateBookData.ts",
    "license:summary": "license-checker --summary> license.summary.txt",
    "commit": "git cz",
    "generate:gh-stars": "python gh-export-stars/src/main.py --user s-hirano-ist --output ./src/content/portfolio/gh-stars.json",
    "prepare": "husky",
    "security": "pnpm audit",
    "prisma:generate": "prisma generate --no-hints --no-engine --schema=./s-private/prisma/schema.prisma",
    "fetch-news": "tsx script/fetchNews.ts",
    "postinstall": "pnpm prisma:generate"
  },
  "lint-staged": {
    "*": [
      "pnpm check",
      "pnpm fmt",
      "pnpm lint:secret",
      "bash checkLicense.sh"
    ],
    "src/**/*.{js, jsx, ts, tsx, astro}": [
      "pnpm tsc",
      "pnpm lint"
    ],
    "_markdown/**/*.md": "pnpm lint:mark",
    "src/**/*.{css, astro}": "pnpm lint:css"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/react": "3.6.3",
    "@astrojs/sitemap": "3.2.1",
    "@astrojs/tailwind": "5.1.5",
    "@fontsource-variable/noto-sans-jp": "5.1.1",
    "@googleapis/books": "4.0.0",
    "astro": "4.16.18",
    "fuse.js": "7.0.0",
    "github-slugger": "2.0.0",
    "jsdom": "26.0.0",
    "lenis": "1.1.22",
    "mdast-util-toc": "7.1.0",
    "photoswipe": "5.4.4",
    "rehype-autolink-headings": "7.1.0",
    "rehype-slug": "6.0.0",
    "rehype-stringify": "10.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.1",
    "remark-toc": "9.0.0",
    "sanitize-html": "2.14.0",
    "satori": "0.12.1",
    "sharp": "0.33.5",
    "tailwindcss": "3.4.17",
    "tailwindcss-animated": "2.0.0",
    "taos": "1.0.5",
    "theme-change": "2.5.0",
    "typescript": "5.5.4",
    "unified": "11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "9.20.0",
    "@playwright/test": "1.49.1",
    "@prisma/client": "6.3.1",
    "@secretlint/secretlint-rule-preset-recommend": "9.0.0",
    "@tailwindcss/typography": "0.5.16",
    "@types/jsdom": "21.1.7",
    "@types/mdast": "4.0.4",
    "@types/react": "19.0.10",
    "@types/sanitize-html": "2.13.0",
    "astro-eslint-parser": "1.1.0",
    "daisyui": "5.0.0",
    "dotenv": "16.4.7",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.2",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-import-x": "4.6.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-redos": "4.4.5",
    "eslint-plugin-tailwindcss": "3.17.5",
    "git-cz": "4.9.0",
    "husky": "9.1.7",
    "license-checker": "25.0.1",
    "lint-staged": "15.3.0",
    "markdownlint-cli2": "0.17.2",
    "postcss-html": "1.7.0",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "prisma": "6.3.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "secretlint": "9.0.0",
    "stylelint": "16.12.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-no-unsupported-browser-features": "8.0.4",
    "stylelint-order": "6.0.4",
    "svgo": "3.3.2",
    "ts-node": "10.9.2",
    "tsx": "4.19.3",
    "typescript-eslint": "8.23.0",
    "vnu-jar": "24.10.17"
  }
}
