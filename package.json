{
  "name": "s-public",
  "version": "1.5.0",
  "type": "module",
  "description": "Portfolio and blog pages of s-hirano-ist.",
  "author": "s-hirano-ist",
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "check": "astro check",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "tsc": "tsc --noEmit",
    "fmt": "prettier --cache --check **/*.astro .",
    "fmt:fix": "prettier --cache --write **/*.astro .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:mark": "markdownlint-cli2 src/content/summary/*",
    "lint:mark:fix": "markdownlint-cli2-fix src/content/summary/*",
    "lint:css": "stylelint .",
    "lint:css:fix": "stylelint . --fix",
    "lint:secret": "secretlint --maskSecrets **/*",
    "snapshots": "playwright test",
    "snapshots:update": "playwright test --update-snapshots",
    "generate:photo": "python script/generate_photo_path.py",
    "generate:book": "node --loader ts-node/esm script/generateBookData.ts",
    "commit": "git cz",
    "generate:gh-stars": "python gh-export-stars/src/main.py --user s-hirano-ist --output ./src/content/portfolio/gh-stars.json",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "pnpm check",
      "pnpm fmt",
      "pnpm lint:secret"
    ],
    "src/**/*.{js, jsx, ts, tsx, astro}": [
      "pnpm tsc",
      "pnpm lint"
    ],
    "_markdown/**/*.md": "pnpm lint:mark",
    "src/**/*.{css, astro}": "pnpm lint:css",
    "src/**/*": "python3 script/generate_photo_path.py"
  },
  "dependencies": {
    "@astrojs/check": "0.8.2",
    "@astrojs/react": "3.6.0",
    "@astrojs/sitemap": "3.1.6",
    "@astrojs/tailwind": "5.1.0",
    "@astrojs/vercel": "7.7.2",
    "@fontsource-variable/noto-sans-jp": "5.0.19",
    "@googleapis/books": "4.0.0",
    "@vercel/speed-insights": "1.0.12",
    "astro": "4.12.2",
    "fuse.js": "7.0.0",
    "github-slugger": "2.0.0",
    "jsdom": "24.1.0",
    "lenis": "1.1.6",
    "mdast-util-toc": "7.1.0",
    "photoswipe": "5.4.4",
    "rehype-autolink-headings": "7.1.0",
    "rehype-slug": "6.0.0",
    "rehype-stringify": "10.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "remark-toc": "9.0.0",
    "sanitize-html": "2.13.0",
    "satori": "0.10.14",
    "sharp": "0.33.4",
    "tailwindcss": "3.4.6",
    "taos": "1.0.5",
    "theme-change": "2.5.0",
    "typescript": "5.5.3",
    "unified": "11.0.5"
  },
  "devDependencies": {
    "@playwright/test": "1.45.2",
    "@secretlint/secretlint-rule-preset-recommend": "8.2.4",
    "@tailwindcss/typography": "0.5.13",
    "@types/jsdom": "21.1.7",
    "@types/mdast": "4.0.4",
    "@types/react": "18.3.3",
    "@types/sanitize-html": "2.11.0",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "astro-eslint-parser": "1.0.2",
    "daisyui": "4.12.10",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-astro": "1.2.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-redos": "4.4.5",
    "eslint-plugin-tailwindcss": "3.17.4",
    "git-cz": "4.9.0",
    "husky": "9.1.1",
    "lint-staged": "15.2.7",
    "markdownlint-cli2": "0.13.0",
    "postcss-html": "1.7.0",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "secretlint": "8.2.4",
    "stylelint": "16.7.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-no-unsupported-browser-features": "8.0.1",
    "stylelint-order": "6.0.4",
    "ts-node": "10.9.2",
    "vnu-jar": "23.4.11"
  }
}
