{
  "name": "s-contents-summary",
  "version": "1.0.0",
  "author": "s-hirano-ist",
  "description": "Extract data from db and write to file.",
  "type": "module",
  "scripts": {
    "biome:ci": "biome ci --config-path=biome.json",
    "biome:fix": "bun biome check --write  --config-path=biome.json",
    "mark:check": "markdownlint-cli2 **/*.md",
    "mark:fix": "markdownlint-cli2 **/*.md --fix",
    "generate:photo": "bun script/generate-photo-path.ts",
    "generate:book": "bun script/generate-book-data.ts",
    "generate:gh-stars": "python gh-export-stars/src/main.py --user s-hirano-ist --output ./portfolio/gh-stars.json",
    "prisma:generate": "prisma generate --no-hints --no-engine --schema=./s-schema/schema.prisma",
    "postinstall": "bun prisma:generate"
  },
  "dependencies": {
    "@prisma/client": "6.11.1",
    "@googleapis/books": "6.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@types/bun": "1.2.18",
    "markdownlint-cli2": "0.18.1"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  }
}
